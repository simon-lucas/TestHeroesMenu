package com.studioshitless.views{import flash.events.MouseEvent;import com.studioshitless.events.IButtonEvent;import flash.utils.setTimeout;import flash.display.MovieClip;public class StandardButton extends MovieClip implements IButton{	public var buttonID:uint;		public function StandardButton()	{		super();		addEventListener(MouseEvent.MOUSE_DOWN,clicked)		gotoAndStop(1)	}		public function clicked(e:MouseEvent):void{		setHilightOn()		setTimeout(delayEvent,200)	}		public function delayEvent():void{		setHilightOff()		var evt:IButtonEvent = new IButtonEvent(IButtonEvent.BUTTONCLICKED)		evt.buttonID = buttonID;			dispatchEvent(evt);	}		public function setOff():void{		trace("setOff", buttonID)		alpha=1;	}	public function setOn():void{		trace("setOn", buttonID)		alpha=0.2;		}		public function setHilightOff():void{		gotoAndStop(1)	}	public function setHilightOn():void{		gotoAndStop(2)	}	}}